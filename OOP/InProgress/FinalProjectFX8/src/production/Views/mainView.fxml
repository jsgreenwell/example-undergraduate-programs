<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<TabPane fx:id="mainTabViews" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="production.Controllers.Controller">
   <tabs>
      <Tab fx:id="rProductionTab" text="Record Production">
        <content>
          <!-- Add the new fxml here and change controller -->
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <ComboBox fx:id="itemTypeCmb" layoutX="30.0" layoutY="28.0" onMouseClicked="#fillComboBox" prefHeight="22.0" prefWidth="163.0" promptText="Click to Select Item Type" />
               </children></AnchorPane>
        </content>
      </Tab>
      <Tab text="Add Product">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
      <Tab text="Reports">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
   </tabs>
</TabPane>
